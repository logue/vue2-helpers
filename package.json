{
  "name": "@logue/vue2-helpers",
  "version": "2.1.3",
  "type": "module",
  "description": "A util package to use Vue 2 with Composition API easily",
  "keywords": [
    "vue2",
    "vue2.7",
    "utils",
    "helpers",
    "composition",
    "vuex",
    "vue-router",
    "vuetify",
    "teleport"
  ],
  "author": {
    "name": "Ambit Tsai",
    "email": "ambit_tsai@qq.com"
  },
  "contributors": [
    {
      "name": "Logue",
      "url": "https://logue.dev/",
      "email": "logue@hotmail.co.jp"
    }
  ],
  "license": "Apache-2.0",
  "main": "index.umd.js",
  "module": "index.es.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./index.es.js",
      "require": "./index.umd.js",
      "types": "./index.d.ts"
    },
    "./vuex": {
      "import": "./vuex.es.js",
      "require": "./vuex.umd.js",
      "types": "./vuex.d.ts"
    },
    "./vue-router": {
      "import": "./vue-router.es.js",
      "require": "./vue-router.umd.js",
      "types": "./vue-router.d.ts"
    },
    "./vuetify": {
      "import": "./vuetify.es.js",
      "require": "./vuetify.umd.js",
      "types": "./vuetify.d.ts"
    },
    "./teleport": {
      "import": "./teleport.es.js",
      "require": "./teleport.umd.js",
      "types": "./teleport.d.ts"
    }
  },
  "files": [
    "./*.d.ts",
    "./*.d.ts.map",
    "./*.es.js",
    "./*.es.js.map",
    "./*.umd.js",
    "./*.umd.js.map"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18.15.0",
    "yarn": ">=1.22.19"
  },
  "packageManager": "yarn@3.4.1",
  "scripts": {
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . -w -u",
    "build": "node scripts/build.js && tsc --declaration --emitDeclarationOnly",
    "build:clean": "node scripts/clean.js",
    "prepare": "husky install",
    "postinstall": "npx vue-demi-fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/logue/vue2-helpers.git"
  },
  "homepage": "https://github.com/logue/vue2-helpers#readme",
  "bugs": {
    "url": "https://github.com/logue/vue2-helpers/issues"
  },
  "dependencies": {
    "vue-demi": "latest"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.7.1",
    "vue": "^2.0.0 || <3.0.0",
    "vue-router": "^3.6.5",
    "vuetify": "^2.0.0 || <3.0.0",
    "vuex": "^3.6.2"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    },
    "vuetify": {
      "optional": true
    },
    "vuex": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.36.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^40.0.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.4",
    "rimraf": "^4.4.0",
    "rollup": "^3.19.1",
    "typescript": "^4.9.5",
    "vue": "^2.7.14",
    "vue-router": "^3.6.5",
    "vue-template-compiler": "^2.7.14",
    "vuetify": "^2.6.14",
    "vuex": "^3.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "json5": "^2.2.3",
    "http-cache-semantics": "^4.1.1"
  }
}
