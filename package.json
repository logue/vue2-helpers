{
  "name": "@logue/vue2-helpers",
  "version": "1.2.1",
  "description": "A util package to use Vue 2 with Composition API easily",
  "author": {
    "name": "Ambit Tsai",
    "email": "ambit_tsai@qq.com"
  },
  "contributors": [
    {
      "name": "Logue",
      "url": "https://logue.dev/",
      "email": "logue@hotmail.co.jp"
    }
  ],
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=16.15.1",
    "yarn": ">=1.22.4"
  },
  "files": [
    "/dist"
  ],
  "packageManager": "yarn@3.2.1",
  "scripts": {
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . -w -u",
    "build": "node scripts/build.js && vue-tsc --declaration --emitDeclarationOnly",
    "build:clean": "rimraf dist",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/logue/vue2-helpers.git"
  },
  "homepage": "https://github.com/logue/vue2-helpers#readme",
  "bugs": {
    "url": "https://github.com/logue/vue2-helpers/issues"
  },
  "peerDependencies": {
    "vue": "^2.7.0",
    "vue-router": "^3.5.4",
    "vuetify": "^2.6.7",
    "vuex": "^3.6.2"
  },
  "peerDependenciesMeta": {
    "vuetify": {
      "optional": true
    },
    "vuex": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/eslint": "^8.4.5",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.3",
    "@typescript-eslint/parser": "^5.30.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "eslint": "^8.19.0",
    "eslint-config-google": "^0.14.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.1.4",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-json-schema-validator": "^3.1.0",
    "eslint-plugin-jsonc": "^2.3.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.7",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.7.4",
    "vue": "^2.7.0",
    "vue-eslint-parser": "^9.0.3",
    "vue-router": "^3.5.4",
    "vue-tsc": "^0.38.2",
    "vuetify": "^2.6.7",
    "vuex": "^3.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.vue": "vue-tsc --noEmit",
    "*.{js,ts,json,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "prettier": "^2.7.1"
  }
}
